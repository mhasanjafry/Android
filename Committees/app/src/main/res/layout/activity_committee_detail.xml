<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/activity_committee_detail"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="@dimen/activity_vertical_margin"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
tools:context="com.mobilesiri.volleycustomlistview.CommitteeDetail">

<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:text="TextView"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/billidval"
            android:textSize="20sp"
            android:textColor="@color/text"
            android:layout_marginTop="16dp"
            android:gravity="right"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/billid"
            android:layout_alignBaseline="@+id/billidval"
            android:layout_alignParentLeft="true"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:textSize="20sp" />

        <TextView
            android:text="TextView"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/titleval"
            android:textSize="20sp"
            android:textColor="@color/text"
            android:layout_marginTop="16dp"
            android:gravity="right"
            android:layout_below="@+id/billidval"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/title"
            android:textSize="20sp"
            android:layout_alignBaseline="@+id/titleval"
            android:layout_alignParentLeft="true"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:layout_alignStart="@+id/billid" />

        <TextView
            android:text="TextView"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/typeval"
            android:textSize="20sp"
            android:textColor="@color/text"
            android:layout_marginTop="16dp"
            android:gravity="right"
            android:layout_below="@+id/titleval"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/type"
            android:textSize="20sp"
            android:layout_alignBaseline="@+id/typeval"
            android:layout_alignParentLeft="true"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:layout_alignStart="@+id/billid" />

        <TextView
            android:text="TextView"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/sponsorval"
            android:textSize="20sp"
            android:textColor="@color/text"
            android:layout_marginTop="16dp"
            android:gravity="right"
            android:layout_below="@+id/typeval"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/sponsor"
            android:textSize="20sp"
            android:layout_alignBaseline="@+id/sponsorval"
            android:layout_alignParentLeft="true"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:layout_alignStart="@+id/billid" />

        <TextView
            android:text="TextView"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/chamberval"
            android:textSize="20sp"
            android:textColor="@color/text"
            android:layout_marginTop="16dp"
            android:gravity="right"
            android:layout_below="@+id/sponsorval"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/chamber"
            android:textSize="20sp"
            android:layout_alignBaseline="@+id/chamberval"
            android:layout_alignParentLeft="true"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:layout_alignStart="@+id/billid" />

        <TextView
            android:text="TextView"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/statusval"
            android:textSize="20sp"
            android:textColor="@color/text"
            android:layout_marginTop="16dp"
            android:gravity="right"
            android:layout_below="@+id/chamberval"
            android:layout_alignParentRight="true"
            android:paddingRight="@dimen/activity_horizontal_margin" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/status"
            android:textSize="20sp"
            android:layout_alignBaseline="@+id/statusval"
            android:layout_alignParentLeft="true"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:layout_alignStart="@+id/billid" />

    </RelativeLayout>

</ScrollView>

</RelativeLayout>
